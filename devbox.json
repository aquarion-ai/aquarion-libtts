        "AQ_PIP_MINIMUM_VERSION": "25.1.0",
        "AQ_PRE_COMMIT_MINIMUM_VERSION": "4.2.0",
        "AQ_PYTHON_MINIMUM_VERSION": "3.13.3",
        "AQ_REUSE_MINIMUM_VERSION": "5.0.2",
        "PATH": "$PWD/scripts/:$PATH",
        "UV_NO_SYNC": "true"
